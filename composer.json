{
    "name": "tholcomb/symple-webpack-encore",
    "description": "Webpack Encore for Symple",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        },
        {
            "name": "Tyler Holcomb",
            "email": "tyler@tholcomb.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Symfony\\WebpackEncoreBundle\\": "src/Symfony/",
            "Tholcomb\\Symple\\WebpackEncore\\": "src/Tholcomb/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\WebpackEncoreBundle\\Tests\\": "tests/Symfony/",
            "Tholcomb\\Symple\\WebpackEncore\\Tests\\": "tests/Tholcomb/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "symfony/asset": "^3.4 || ^4.0 || ^5.0",
        "symfony/config": "^3.4 || ^4.0 || ^5.0",
        "pimple/pimple": "^3.2",
        "symfony/http-kernel": "^3.4 || ^4.0 || ^5.0",
        "symfony/service-contracts": "^1.0 || ^2.0",
        "tholcomb/rw-base": "dev-master",
        "tholcomb/rw-twig": "dev-master",
        "tholcomb/rw-http": "dev-master"
    },
    "require-dev": {
        "symfony/cache": "^3.4 || ^4.0 || ^5.0",
        "symfony/phpunit-bridge": "^4.3.5 || ^5.0",
        "symfony/twig-bundle": "^3.4 || ^4.0 || ^5.0",
        "symfony/web-link": "^3.4 || ^4.0 || ^5.0"
    },
    "extra": {
        "thanks": {
            "name": "symfony/webpack-encore",
            "url": "https://github.com/symfony/webpack-encore"
        }
    }
}
